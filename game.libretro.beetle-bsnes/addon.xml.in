<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.beetle-bsnes"
		name="Nintendo - SNES / SFC (Beetle bsnes)"
		version="0.9.26.27"
		provider-name="byuu, Mednafen Team">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
		<import addon="game.controller.snes" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms>SNES</platforms>
		<extensions>smc|fig|bs|st|sfc</extensions>
		<supports_vfs>false</supports_vfs>
		<supports_standalone>false</supports_standalone>
		<requires_opengl>false</requires_opengl>
	</extension>
	<extension point="xbmc.addon.metadata">
